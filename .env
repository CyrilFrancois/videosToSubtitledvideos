# REQUIRED: Your OpenAI API key for Translation
OPENAI_API_KEY=[YOUR GPT KEY HERE]

# Folder to your Media/Video directory
NEXT_PUBLIC_MEDIA_PATH=C:/Videos

# The referential language dictionary
NEXT_PUBLIC_LANGUAGES='{"English":"en", "French":"fr", "Spanish":"es", "German":"de", "Japanese":"ja", "Italian":"it", "Portuguese":"pt"}'

# OPTIONAL: Whisper model size (tiny, base, small, medium, large)
WHISPER_MODEL=base

# NEXT_PUBLIC_ variables are baked into the frontend build
NEXT_PUBLIC_API_URL=http://localhost:8000

